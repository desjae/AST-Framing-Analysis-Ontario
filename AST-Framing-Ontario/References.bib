@article{marziChildrenIndependentMobility2018,
  title = {Children's {{Independent Mobility}}: {{Current Knowledge}}, {{Future Directions}}, and {{Public Health Implications}}},
  shorttitle = {Children's {{Independent Mobility}}},
  author = {Marzi, Isabel and Reimers, Anne Kerstin},
  year = {2018},
