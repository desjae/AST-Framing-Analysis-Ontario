}

@article{villanuevaWhereChildrenTravel2013,
  title = {Where {{Do Children Travel}} to and {{What Local Opportunities Are Available}}? {{The Relationship Between Neighborhood Destinations}} and {{Children}}'s {{Independent Mobility}}},
  shorttitle = {Where {{Do Children Travel}} to and {{What Local Opportunities Are Available}}?},
  author = {Villanueva, Karen and {Giles-Corti}, Billie and Bulsara, Max and Timperio, Anna and McCormack, Gavin and Beesley, Bridget and Trapp, Georgina and Middleton, Nicholas},
  year = {2013},
  volume = {45},
  pages = {679--705},
  publisher = {{SAGE Publications}},
  issn = {0013-9165},
  doi = {10.1177/0013916512440705},
  abstract = {Associations between access to local destinations and children's independent mobility (IM) were examined. In 2007, 10- to 12-year-olds (n = 1,480) and their parents (n = 1,314) completed a survey. Children marked on a map the destinations they walked or cycled to (n = 1,132), and the availability of local destinations was assessed using Geographic Information Systems. More independently mobile children traveled to local destinations than other children. The odds of IM more than halved in both boys and girls whose parents reported living on a busy road (boys, OR = 0.48; girls, OR = 0.36) and in boys who lived near shopping centers (OR = 0.18) or community services (OR = 0.25). Conversely, the odds of IM more than doubled in girls living in neighborhoods with well-connected low-traffic streets (OR = 2.32) and increased in boys with access to local recreational (OR = 1.67) and retail (OR = 1.42) destinations. Creating safe and accessible places and routes may facilitate children's IM, partly by shaping parent's and children's feelings of safety while enhancing their confidence in the child's ability to use active modes without an adult.},
  journal = {Environment and Behavior},
  keywords = {children,destinations,independent mobility,neighborhood},
  number = {6}
}

@article{vlaarRoamingNeighbourhoodInfluences2019,
  title = {Roaming the {{Neighbourhood}}: {{Influences}} of {{Independent Mobility Parenting Practices}} and {{Parental Perceived Environment}} on {{Children}}'s {{Territorial Range}}},
  shorttitle = {Roaming the {{Neighbourhood}}},
  author = {Vlaar, Janae and Brussoni, Mariana and Janssen, Ian and M{\^a}sse, Louise C.},
  year = {2019},
  month = aug,
  volume = {16},
  issn = {1660-4601},
  doi = {10.3390/ijerph16173129},
  abstract = {Children's independent mobility (IM), their freedom to move about their neighbourhood without supervision by adults, has been in steady decline in recent decades. Previous research has linked perceptions of the environment with various measures of IM, but recently concerns have been raised regarding inconsistency in measuring IM. This study used various measures of IM and aimed to address how parental perceptions of the neighbourhood environment are associated with children's territorial range (actual IM), as well as how this relationship is mediated by IM parenting practices (allowed IM). A sample of 105 child/parent dyads from Vancouver, Canada participated in this study. Children (age 10-13) wore a global positioning system (GPS) watch and an accelerometer and completed an activity diary for seven days to assess their territorial range. Parents completed a questionnaire that assessed perceptions of their neighbourhood environment and IM parenting practices-license for IM and roaming allowance. Path analyses were used to address the research aims. License for IM and roaming allowance mediated the relationship between perceived walking facilities, crime safety, and neighbourhood relations and children's territorial range. Findings suggest that future interventions to increase children's territorial range should focus primarily on attitude and behaviour change among parents to grant children more freedom.},
  file = {C\:\\Users\\desjae\\Zotero\\storage\\N2WLHTDB\\Vlaar et al. - 2019 - Roaming the Neighbourhood Influences of Independe.pdf},
  journal = {International Journal of Environmental Research and Public Health},
  keywords = {activity spaces,Adolescent,Adult,British Columbia,Child,Female,Geographic Information Systems,Humans,independent mobility,Male,neighbourhood activity,Parenting,Parents,Residence Characteristics,Surveys and Questionnaires,Walking},
  language = {eng},
  number = {17},
  pmcid = {PMC6747532},
  pmid = {31466293}
}

