@article{rothmanActiveSchoolTransportation2021,
  title = {Active School Transportation and the Built Environment across {{Canadian}} Cities: {{Findings}} from the Child Active Transportation Safety and the Environment ({{CHASE}}) Study},
  shorttitle = {Active School Transportation and the Built Environment across {{Canadian}} Cities},
  author = {Rothman, Linda and Hagel, Brent and Howard, Andrew and Cloutier, Marie Soleil and Macpherson, Alison and Aguirre, Alberto Nettel and McCormack, Gavin R. and Fuselli, Pamela and Buliung, Ron and HubkaRao, Tate and Ling, Rebecca and Zanotto, Moreno and Rancourt, Mathieu and Winters, Meghan},
  year = {2021},
