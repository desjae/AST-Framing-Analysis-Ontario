}

@article{rutbergChildrenMotivationOvercame2019,
  title = {Children's Motivation Overcame Parental Hesitation: Active School Transportation in {{Sweden}}},
  shorttitle = {Children's Motivation Overcame Parental Hesitation},
  author = {Rutberg, Stina and Lindqvist, Anna-Karin},
