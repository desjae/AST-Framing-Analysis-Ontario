@article{scheinerChildrenModeChoice2019,
  title = {Children's Mode Choice for Trips to Primary School: A Case Study in {{German}} Suburbia},
  shorttitle = {Children's Mode Choice for Trips to Primary School},
  author = {Scheiner, Joachim and Huber, Oliver and Lohm{\"u}ller, Stefan},
