}

@article{priceSchoolAdministratorsPerceptions2011,
  title = {School Administrators' Perceptions of Factors That Influence Children's Active Travel to School},
  author = {Price, Anna E. and Pluto, Delores M. and Ogoussan, Olga and Banda, Jorge A.},
  year = {2011},
